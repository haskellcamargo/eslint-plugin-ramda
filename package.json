{
  "name": "eslint-plugin-ramda",
  "version": "0.1.0",
  "description": "Ramda refactoring and simplification",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "ramda",
    "fantasy-land"
  ],
  "author": "Marcelo Camargo <marcelocamargo@linuxmail.org>",
  "main": "index.js",
  "scripts": {
    "lint": "xo",
    "test": "nyc ava",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov"
  },
  "dependencies": {
    "create-eslint-index": "^1.0.0",
    "req-all": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.23.0",
    "codecov": "^3.0.0",
    "eslint": "~4.11.0",
    "eslint-ava-rule-tester": "^2.0.2",
    "nyc": "^11.3.0",
    "xo": "^0.18.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "xo": {
    "esnext": true,
    "space": 4,
    "rules": {
      "operator-linebreak": [
        "error",
        "before"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ]
    }
  }
}
